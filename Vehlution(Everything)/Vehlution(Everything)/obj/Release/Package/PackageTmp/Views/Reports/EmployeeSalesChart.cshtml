@model Dictionary<string, double>
@{
    Layout = null;
}

@{
    var myChart = new Chart(width: 400, height: 250)
        .AddTitle("Car Sales per Employee")
        .AddSeries(
            name: "Employee",
            xValue: Model.Keys,
            yValues: Model.Values)
        .Write();
}

<div style="text-align:center">
    <input type="button" value="Home" class="btn btn-secondary" onclick="location.href='@Url.Action("AdminNav","Nav")'" />
</div>